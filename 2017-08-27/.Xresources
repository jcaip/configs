
! special
*.foreground:   #515253
*.background:   #fcfdfe
*.cursorColor:  #515253

! black
*.color0:       #0c0d0e
*.color8:       #737475

! red
*.color1:       #e31a1c
*.color9:       #e31a1c

! green
*.color2:       #31a354
*.color10:      #31a354

! yellow
*.color3:       #dca060
*.color11:      #dca060

! blue
*.color4:       #3182bd
*.color12:      #3182bd

! magenta
*.color5:       #756bb1
*.color13:      #756bb1

! cyan
*.color6:       #80b1d3
*.color14:      #80b1d3

! white
*.color7:       #b7b8b9
*.color15:      #fcfdfe


! ------------------------------------------------------------------------------
! --- end of terminal colors section -------------------------------------------
! ------------------------------------------------------------------------------
URxvt.scrollBar: false
Xft.rgba:none
Xft.hinting:true
Xft.hintstyle:hintfull
!Xft.dpi:140

URxvt.font: xft:Ubuntu Mono derivative Powerline:size=11
URxvt.letterSpace:0

URxvt.transparent: false
URxvt.shading: 40

Xcursor.theme: xcursor-simpleandsoft
Xcursor.size: 16

urxvt*perl-ext-common:        default,matcher
urxvt*url-launcher:        /usr/sbin/google-chrome-unstable
urxvt*matcher.button:        1
